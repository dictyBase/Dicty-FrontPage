{
  "name": "dicty-frontpage",
  "version": "0.0.0-semantically-released",
  "private": true,
  "engines": {
    "node": "10.4.0"
  },
  "scripts": {
    "prettier": "prettier --version",
    "precommit": "lint-staged",
    "flow": "flow",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test-coverage": "react-scripts test --env=jsdom --coverage",
    "eject": "react-scripts eject",
    "cz": "git-cz",
    "semantic-release": "semantic-release",
    "analyze": "source-map-explorer 'build/static/js/*.js'"
  },
  "dependencies": {
    "@material-ui/core": "^1.4.0",
    "@material-ui/icons": "^1.1.1",
    "connected-react-router": "^6.4.0",
    "core-js": "^3.0.1",
    "dicty-components-header-footer": "github:dictyBase/dicty-components-header-footer#v7.0.0",
    "dicty-components-login": "github:dictyBase/dicty-components-login#v1.4.0",
    "dicty-components-navbar": "github:dictyBase/dicty-components-navbar#v0.2.0",
    "dicty-components-redux": "github:dictybase-playground/dicty-components-redux#v2.3.0",
    "es6-promise": "^4.2.6",
    "font-awesome": "^4.7.0",
    "get-video-id": "^3.1.0",
    "history": "^4.7.2",
    "immutable": "^3.8.2",
    "is-url": "^1.2.4",
    "isomorphic-fetch": "^2.2.1",
    "jwt-decode": "^2.2.0",
    "react": "^16.8.6",
    "react-color": "^2.14.1",
    "react-dom": "^16.8.6",
    "react-fontawesome": "^1.6.1",
    "react-ga": "^2.5.3",
    "react-helmet": "^5.2.0",
    "react-loading-skeleton": "^1.1.1",
    "react-redux": "^6.0.1",
    "react-responsive-carousel": "^3.1.41",
    "react-router-dom": "^5.0.0",
    "react-scripts": "^3.0.0",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.5",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.13.2",
    "slate": "^0.34.5",
    "slate-edit-list": "^0.12.1",
    "slate-edit-table": "^0.18.0",
    "slate-html-serializer": "^0.6.11",
    "slate-react": "^0.13.2",
    "styled-components": "^2.4.0",
    "typeface-lato": "0.0.54",
    "typeface-merriweather": "0.0.72",
    "typeface-montserrat": "0.0.54",
    "typeface-roboto": "0.0.54",
    "typeface-roboto-condensed": "0.0.54",
    "typeface-roboto-mono": "0.0.54",
    "typeface-roboto-slab": "0.0.54"
  },
  "devDependencies": {
    "@semantic-release/changelog": "^3.0.2",
    "@semantic-release/git": "^7.0.8",
    "commitizen": "^3.1.1",
    "cz-conventional-changelog": "^2.1.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "flow-bin": "^0.80.0",
    "flow-typed": "^2.5.1",
    "husky": "^2.1.0",
    "jest-enzyme": "^7.0.2",
    "jest-styled-components": "^6.3.1",
    "lint-staged": "^8.1.5",
    "prettier": "^1.17.0",
    "react-styleguidist": "^9.0.9",
    "react-test-renderer": "^16.8.6",
    "semantic-release": "^15.13.12",
    "source-map-explorer": "^1.8.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --no-semi --trailing-comma all --jsx-bracket-same-line true --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "release": {
    "noCi": true,
    "tagFormat": "${version}",
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/git"
    ],
    "prepare": [
      "@semantic-release/changelog"
    ],
    "publish": [
      "@semantic-release/github"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
